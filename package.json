{
  "name": "ordini-app",
  "version": "1.0.0",
  "description": "Stores recipies I love",
  "scripts": {
    "test": "mocha shared/tests/*.spec.js --compilers js:babel-register",
    "test:server": "cross-env NODE_ENV=test PORT=8080 MONGO_URL=mongodb://localhost:27017/mern-test mocha --compilers js:babel-register --recursive server/tests/**/*.spec.js",
    "start": "cross-env NODE_ENV=development node index.js",
    "start:prod": "cross-env NODE_ENV=production node index.js",
    "bs": "npm run clean && npm run build && npm run start:prod",
    "minify": "cleancss -o static/css/app.min.css static/css/app.css",
    "build": "cross-env NODE_ENV=production webpack --config webpack.config.prod.js && npm run minify",
    "clean": "rimraf static/dist",
    "slate": "rimraf node_modules && npm install",
    "lint": "eslint shared client"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/vivanov1410/ordini-app.git"
  },
  "bugs": {
    "url": "https://github.com/vivanov1410/ordini-app.git"
  },
  "license": "MIT",
  "dependencies": {
    "babel-core": "^6.5.2",
    "body-parser": "^1.15.0",
    "classnames": "^2.2.3",
    "cuid": "^1.3.8",
    "express": "^4.13.4",
    "history": "^2.0.0",
    "http-status": "^0.2.0",
    "isomorphic-fetch": "^2.2.1",
    "mongoose": "^4.4.4",
    "react": "^0.14.7",
    "react-dom": "^0.14.7",
    "react-redux": "^4.4.0",
    "react-router": "^2.0.0",
    "redux": "^3.3.1",
    "redux-logger": "^2.6.0",
    "redux-thunk": "^1.0.3",
    "sanitize-html": "^1.11.3",
    "slug": "^0.9.1"
  },
  "devDependencies": {
    "babel-eslint": "^5.0.0",
    "babel-loader": "^6.2.3",
    "babel-plugin-react-transform": "^2.0.0",
    "babel-polyfill": "^6.5.0",
    "babel-preset-es2015": "^6.5.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-react-hmre": "^1.1.0",
    "babel-preset-stage-1": "^6.5.0",
    "babel-register": "^6.5.2",
    "chai": "^3.5.0",
    "clean-css": "^3.4.9",
    "cross-env": "^1.0.7",
    "css-loader": "^0.23.1",
    "css-modules-require-hook": "^2.1.0",
    "deep-freeze": "0.0.1",
    "eslint": "^2.2.0",
    "eslint-config-airbnb": "^6.0.1",
    "eslint-plugin-react": "^4.0.0",
    "expect": "^1.14.0",
    "expect-jsx": "^2.3.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "mocha": "^2.4.5",
    "react-addons-test-utils": "^0.14.7",
    "react-transform-hmr": "^1.0.2",
    "rimraf": "^2.5.2",
    "style-loader": "^0.13.0",
    "supertest": "^1.2.0",
    "webpack": "^1.12.13",
    "webpack-dev-middleware": "^1.5.1",
    "webpack-hot-middleware": "^2.7.1"
  },
  "engines": {
    "node": ">=4"
  }
}
